
@model Sids.Prodesp.Model.Interface.Reserva.IReserva

<fieldset class="fsStyles">
    <legend>
        <label class="label-form-field"><span class="text-danger"></span>Autorizado Por:</label>
    </legend>
    <div class="row form-group">
        <div class="col-sm-12 col-lg-2 col-md-2  form-group-sm form-group has-feedback"  style="width:12%">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Cod. Assinatura</label>
            <div class="input-group-sm">
                @Html.TextBoxFor(model => model.AutorizadoAssinatura, new { @class = "form-control prodesp number", @maxlength = "5", @id= "CodAssAutorizado" })
            </div>
        </div>
        <div class="col-sm-12 col-lg-1 col-md-1  ">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Grupo</label>
            @Html.TextBoxFor(model => model.AutorizadoGrupo, new { @class = "form-control", @maxlength = "3", @ReadOnly = "True" , @id = "txtAutorizadoGrupo" })
        </div>
        <div class="col-sm-12 col-lg-2 col-md-1 " style="width:10%">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Orgão</label>
            @Html.TextBoxFor(model => model.AutorizadoOrgao, new { @class = "form-control", @maxlength = "3", @ReadOnly = "True" , @id = "txtAutorizadoOrgao" })
        </div>
        <div class="col-sm-12 col-lg-4 col-md-4  ">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Nome</label>
            @Html.TextBoxFor(model => model.NomeAutorizadoAssinatura, new { @class = "form-control", @maxlength = "55", @ReadOnly = "True" , @id = "txtAutorizadoNome" })
        </div>
        <div class="col-sm-12 col-lg-4 col-md-4 ">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Cargo</label>
            @Html.TextBoxFor(model => model.AutorizadoCargo, new { @class = "form-control", @maxlength = "55", @ReadOnly = "True" , @id = "txtAutorizadoCargo" })
        </div>
    </div>
</fieldset>

