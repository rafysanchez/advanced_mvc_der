            </div>
        </div>
        <div class="col-sm-12 col-lg-1 col-md-1  form-group-sm form-group">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Grupo</label>
            @Html.TextBoxFor(model => model.ExaminadoGrupo, new { @class = "form-control prodesp", @maxlength = "3", @ReadOnly = "True" , @id = "txtExaminadoGrupo"})
        </div>
        <div class="col-sm-12 col-lg-1 col-md-1  form-group-sm" style="width:10%">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Orgão</label>
            @Html.TextBoxFor(model => model.ExaminadoOrgao, new { @class = "form-control prodesp", @maxlength = "3", @ReadOnly = "True" , @id = "txtExaminadoOrgao" })
        </div>
        <div class="col-sm-12 col-lg-4 col-md-4  form-group-sm form-group">
            <label class="control-label label-form-DER"><span class="text-danger"></span>Nome</label>
