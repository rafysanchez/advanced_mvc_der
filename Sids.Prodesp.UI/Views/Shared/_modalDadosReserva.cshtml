<div class="modal fade scroll" id="modalDadosReserva" tabindex="-1" role="dialog" data-focus-on="input:first" aria-labelledby="modalConsultaContrato-label">
    <div class="modal-dialog modal-md" role="document">
        <div class="modal-content scroll">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title" id="modalConsultaContrato-label">Dados da Reserva</h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Nº da Reserva</label>
                        <input id="txtNumReserva" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Nº do Processo</label>
                        <input id="txtNumProcesso" type="text" class="form-control" readonly="True">
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-8">
                        <label class="control-label">CFP / CED</label>
                        <input id="txtCfp" type="text" class="form-control cfp" readonly="True" />
                    </div>
                    <div class="col-md-4">
                        <label class="control-label">Código da Aplicação</label>
                        <input id="txtNumAplicacao" type="text" class="form-control apl" readonly="True" />
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-3">
                        <label class="control-label">Data da Emissão</label>
                        <input id="txtDataEmissao" type="text" class="form-control data" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Previsão Inicial</label>
                        <input id="txtPrevisaoInicio" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Destino do Recurso</label>
                        <input id="txtDestino" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Código da Obra</label>
                        <input id="txtCodObra" type="text" class="form-control" readonly="True" />
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-3">
                        <label class="control-label">Valor</label>
                        <input id="txtValor" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Nº do Contrato</label>
                        <input id="txtNumContrato" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Nº Seq. TAM.</label>
                        <input id="txtSeqTam" type="text" class="form-control" readonly="True" />
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-3">
                        <label class="control-label">Valor do Reforço</label>
                        <input id="txtValorReforco" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Valor Anulado</label>
                        <input id="txtValorAnulado" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Valor Empenhado</label>
                        <input id="txtValorEmprenhado" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Saldo da Reserva</label>
                        <input id="ValorReserva" type="text" class="form-control" readonly="True" />
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-12">
                        <h3 class="margT10 tituloItensPagina">Saldos</h3>
                        <hr class="margT0">
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-3">
                        <label class="control-label">Quota 1</label>
                        <input id="txtQuota1" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Quota 2</label>
                        <input id="txtQuota2" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Quota 3</label>
                        <input id="txtQuota3" type="text" class="form-control" readonly="True" />
                    </div>
                    <div class="col-md-3">
                        <label class="control-label">Quota 4</label>
                        <input id="txtQuota4" type="text" class="form-control" readonly="True" />
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-12">
                        <label class="control-label">Especificação</label>
                        <textarea id="txtEspecificacao" class="form-control" rows="5" readonly="True" style="font-size: 12px"></textarea>
                    </div>
                </div>
                <div class="row margT10">
                    <div class="col-md-12">
                        <button class="btn btn-sm btn-primary" onclick="dadosContrato('modalDadosReserva')"><i class="fa fa-arrow-left"></i> Voltar</button>
                        <button class="btn btn-sm btn-warning" onclick="ConfirmarReserva()" data-dismiss="modal">Confirmar</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
