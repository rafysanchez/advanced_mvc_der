@model Sids.Prodesp.UI.Areas.PagamentoContaUnica.Models.DadoListaBoletosValorViewModel

@{
    var controller = ViewContext.RouteData.GetRequiredString("controller");
}

<div>
    <div class="row">

        <div class="col-sm-12 col-lg-2 col-md-2  form-group-sm form-group has-feedback prodesp issqn outros">
            @Html.LabelFor(model => model.TotalCredores, htmlAttributes: new { @class = "control-label" })
            <div class="input-group-sm">
                @Html.EditorFor(model => model.TotalCredores, new { htmlAttributes = new { @class = "form-control", @ReadOnly= "ReadOnly" } })
            </div>
        </div>

        <div class="col-sm-12 col-lg-2 col-md-2  form-group-sm form-group has-feedback prodesp issqn outros">
            @Html.LabelFor(model => model.ValorTotalLista, htmlAttributes: new { @class = "control-label" })
            <div class="input-group-sm">
                @Html.EditorFor(model => model.ValorTotalLista, new { htmlAttributes = new { @class = "form-control", @ReadOnly = "ReadOnly" } })
            </div>
        </div>

    </div>
</div>
