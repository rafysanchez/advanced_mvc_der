@using Sids.Prodesp.UI.Areas.PagamentoContaUnica.Models

@model DadoReclassificacaoRetencaoTipoViewModel

@{
    var controller = ViewContext.RouteData.GetRequiredString("controller");
}
<div>
    <div class="row">
        <div class="col-md-12">
            <h3 class="tituloItensPagina">Tipo de Reclassificação / Retenção</h3>
            <hr class="margT10" />
        </div>
    </div>

    <div class="row">
        <div class="col-sm-12 col-lg-2 col-md-1  form-group-sm form-group has-feedback">
            @Html.LabelFor(model => model.NormalEstorno, new { @class = "control-label label-form-DER" })<span class='text-danger'></span>
            <div class="input-group-sm">
                @Html.DropDownListFor(model => model.NormalEstorno, Model.NormalEstornoListItems, "Selecione", new { @class = "form-control lockSiafem" })
            </div>
        </div>
        <div class="col-sm-12 col-lg-3 col-md-1  form-group-sm form-group has-feedback">
            @Html.LabelFor(model => model.ReclassificacaoRetencaoTipoId, new { @class = "control-label label-form-DER" })<span class='text-danger'></span>
            <div class="input-group-sm">
                @Html.DropDownListFor(model => model.ReclassificacaoRetencaoTipoId, Model.ReclassificacaoRetencaoTipoListItems, "Selecione", new { @class = "form-control lockSiafem" })
            </div>
        </div>
    </div>

</div>