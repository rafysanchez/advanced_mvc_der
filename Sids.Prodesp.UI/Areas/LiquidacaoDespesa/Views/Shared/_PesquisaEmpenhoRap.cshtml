
@using Sids.Prodesp.UI.Areas.LiquidacaoDespesa.Models

@model PesqEmpenhoRapViewModel

@{
    var controller = ViewContext.RouteData.GetRequiredString("controller");
}

<div id="EmpenhoRap">
    <div class="row">
        <div class="col-md-12">
            <h3 class="tituloItensPagina">Consultar RAP por Empenho</h3>
            <hr class="margT10" />
        </div>
    </div>

    <div class="row siafem">
        <div class="col-sm-12 col-lg-3 col-md-3 form-group-sm form-group has-feedback">
            @Html.LabelFor(model => model.NumeroEmpenho, new { @class = "control-label label-form-DER" })<span class='text-danger'></span>
            <div class="input-group-sm">
                @Html.TextBoxFor(model => model.NumeroEmpenho, new { @class = "form-control lockProdesp empenho", @maxlength="9"})
            </div>
        </div>


        <div id="divbtnConsultar" class="col-sm-12 col-lg-2 col-md-2 form-group-sm form-group has-feedback">
            <label class="invisible">Consultar</label>
            <div class="input-group-sm">
                <button id="btnConsultar" type="button" class="btn btn-primary btn-sm lockProdesp lockSiafem" onclick="EmpenhoRap();"><i class="fa fa-search"></i>  Consultar</button>
            </div>
        </div>
    </div>
</div>